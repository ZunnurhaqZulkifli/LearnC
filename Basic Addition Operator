#include<stdio.h>

int main()
{
    double height = 10;
    double width = 5.2;
    double perimeter  = 0.0;
    double area = 0.0;

    perimeter = 2.0*(height + width);
    area = width*height;

    printf("Width is : %.2f, Height is : %.2f, Perimeter is :%2.f\n", width, height, perimeter);
    printf("Area is : %.2f\n", area);

    return 0;
}
