#include<stdio.h>
#include<stdbool.h>

int main()
{
    unsigned int a = 50; 0011 0010 <---- Convert Number To Binary
    unsigned int b = 12; 0000 1100 <---- Convert Number To Binary
    int result = 0;

    result = a | b;
    //0011 1110 <---- To Get This Value, You Have To add Both a and b 's binary value.
    printf("result is : %d" , result);

    return 0;
}
