#include<stdio.h>

int main()
{
    enum Allday{Mon = 1,Tue = 2,Wed = 3,Thu = 4,Fri = 5,Sat = 6};
    enum Allday today = 0;


    printf("Enter a number :");
    scanf("%d" , &today);


    switch(today)
    {
    case Mon :
        printf("Today is Monday");
        break;
    case Tue :
        printf("This is Tuesday");
        break;
    case Wed :
        printf("This is Wednesday");
        break;
    case Thu :
        printf("This is Thursday");
        break;

    default :
        printf("Nothingness");
        break;
    }
        return 0;
    }


